{
  "project": {
    "name": "Scrapii v2.0",
    "description": "By Loizzz ü¶ä",
    "type": "SPA (React+TypeScript)",
    "purpose": "ethical_scraping + security_audit",
    "status": "prod_ready",
    "stack": {
      "react": "19.2.0",
      "typescript": "5.8.2",
      "vite": "6.2.0"
    }
  },
  "statistics": {
    "source_files": 11,
    "total_lines": 3602,
    "typescript_lines": 2191,
    "html_css_lines": 904,
    "documentation_lines": 435,
    "config_lines": 72
  },
  "core_modules": {
    "ethical_scraper": {
      "functions": ["validate_robots_txt", "analyze_terms_of_service"],
      "flow": "robots_check ‚Üí terms_check ‚Üí ethical_mode_filter",
      "params": {"url": "string", "ethical_mode": "boolean"},
      "output": {"allowed": "boolean", "reason": "string"}
    },
    "security_analyzer": {
      "functions": ["analyze_security_headers", "detect_vulns", "ssl_eval"],
      "vuln_db": "11+ tech_vulns + 7 js_patterns",
      "headers_analyzed": ["CSP", "HSTS", "XSS", "CTO", "RP", "FO", "ServerExpo", "PwrByExpo"],
      "output": {"security_score": "0-100", "vulns": "array", "headers": "object", "ssl": "object"}
    },
    "tech_detector": {
      "functions": ["detect_technologies", "version_compare"],
      "coverage": "50+ tech (frontend,backend,cms,payment,analytics)",
      "patterns": "regex_based + meta_tag_scan + cdn_detection",
      "output": {"tech_stack": "array of {name,version,current,status}"}
    },
    "ecommerce_analyzer": {
      "functions": ["analyze_ecommerce", "detect_products", "payment_methods"],
      "selectors": "15+ product_selectors + structured_data_parser",
      "output": {"products": "array", "payments": "array", "features": "object"}
    }
  },
  "data_structures": {
    "ScrapedData": {
      "meta": {"title": "string", "desc": "string", "keywords": "string", "og_title": "string", "og_desc": "string"},
      "headings": {"h1": "array", "h2": "array", "h3": "array"},
      "links": "array of {text,href}",
      "images": "array of {src,alt}",
      "tech_stack": "TechInfo array",
      "ecommerce": "EcommerceData",
      "subdomains": "SubdomainData array",
      "policy": {"robots_allowed": "boolean", "terms_restricted": "boolean", "prohibited": "boolean", "ethical_mode": "boolean"},
      "security": {"headers": "object", "ssl": "object", "vulns": "array", "privacy_score": "number"},
      "robots_content": "string",
      "users": {"has_users": "boolean", "access_points": "array"}
    }
  },
  "vulnerability_system": {
    "cve_database": {
      "jQuery": {"versions": ["1.", "2.", "3.0-3.4"], "cve": "CVE-2020-11022", "severity": "high"},
      "React": {"versions": ["15-17"], "cve": "CVE-2019-7580", "severity": "high"},
      "WordPress": {"versions": ["4-5.9"], "cve": "CVE-2022-39986", "severity": "critical"},
      "PHP": {"versions": ["5-7.4"], "cve": "CVE-2023-3247", "severity": "critical"}
    },
    "js_vuln_patterns": {
      "eval": {"regex": "/eval\\s*\\(/gi", "type": "RCE_XSS", "severity": "high", "penalty": 8},
      "document_write": {"regex": "/document\\.write\\s*\\(/gi", "type": "XSS", "severity": "medium", "penalty": 4},
      "innerHTML": {"regex": "/innerHTML\\s*=/gi", "type": "DOM_XSS", "severity": "medium", "penalty": 4},
      "setTimeout": {"regex": "/setTimeout\\s*\\(\\s*['\"]/gi", "type": "CODE_INJ", "severity": "medium", "penalty": 4},
      "location": {"regex": "/location\\s*=\\s*.*\\+/gi", "type": "REDIRECT", "severity": "medium", "penalty": 4},
      "console": {"regex": "/console\\.(log|error|warn)/gi", "type": "INFO_DISC", "severity": "low", "penalty": 1},
      "debug": {"regex": "/debug\\s*=\\s*true/gi", "type": "CONFIG_EXP", "severity": "medium", "penalty": 2}
    },
    "security_headers_eval": {
      "csp": {"check": "default-src+script-src", "valid": "boolean", "content": "string"},
      "hsts": {"check": "max-age>=31536000", "valid": "boolean", "content": "string"},
      "xss": {"check": "mode=block|CSP_equiv", "valid": "boolean"},
      "content_type": {"check": "nosniff", "valid": "boolean"},
      "referrer_policy": {"check": "no-referrer|strict-origin", "valid": "boolean"},
      "frame_options": {"check": "deny|sameorigin", "valid": "boolean"},
      "server_exposed": {"check": "server_header", "exposed": "boolean"},
      "powered_by_exposed": {"check": "x-powered-by", "exposed": "boolean"}
    }
  },
  "privacy_scoring": {
    "calculation": {
      "base_score": 100,
      "ssl_missing": -25,
      "csp_missing": -10,
      "hsts_missing": -8,
      "xss_missing": -8,
      "content_type_missing": -5,
      "vuln_penalty": "sum of severity weights",
      "mixed_content": -6,
      "external_links": "-0.5 * min(links,20)",
      "images_no_alt": "-0.3 * min(images,15)",
      "bonus_secure": "+3 to +5",
      "final_score": "max(base_score - penalties + bonus, 10)"
    },
    "severity_weights": {
      "critical": -20,
      "high": -15,
      "medium": -10,
      "low": -5
    }
  },
  "ethical_mode_logic": {
    "policy_validation": "const scraping_allowed = robots_txt_allowed && terms_allowed; const display_status = ethical_mode ? scraping_allowed : true;",
    "blocking_logic": "if(scraping_prohibited && ethical_mode) { return {blocked:true, reason:'robots.txt|terms_violation'}; }",
    "dynamic_indicators": {
      "ethical_mode_ON": {
        "robots_txt": "‚úì/‚úó based on actual policy",
        "terms": "‚úì/‚úó based on ToS analysis"
      },
      "ethical_mode_OFF": {
        "robots_txt": "‚ö†Ô∏è ignored",
        "terms": "‚ö†Ô∏è ignored",
        "warning": "Mode ethical disabled - restrictions ignored"
      }
    }
  },
  "ui_components": {
    "tab_structure": ["summary", "security", "tech", "ecommerce", "subdomains", "gallery", "json"],
    "vulnerability_card_structure": {
      "header": "tech-name, version, severity-badge",
      "details": "description, line-numbers, recommendation"
    }
  },
  "regex_patterns": {
    "user_detection": ["/login/i", "/register/i", "/sign\\s*in/i", "/sign\\s*up/i", "/profile/i", "/account/i", "/dashboard/i", "/admin/i", "/usuario[s]?/i", "/member[s]?/i"],
    "tech_detection": {
      "frontend": "/react|vue|angular|svelte|ember/i",
      "cms": "/wordpress|drupal|joomla|magento/i",
      "payment": "/stripe|paypal|mercadopago/i"
    }
  },
  "cors_proxy_config": {
    "proxy_setup": "CORS_PROXY = 'https://corsproxy.io/?'; request_url = `${CORS_PROXY}${encodeURIComponent(target_url)}`",
    "error_types": {
      "cors": "CORS_BLOCKED",
      "network": "NETWORK_ERROR",
      "ssl": "SSL_CERT_ERROR",
      "timeout": "REQUEST_TIMEOUT",
      "robots": "ROBOTS_VIOLATION"
    }
  },
  "storage_schema": {
    "localStorage_key": "scrapedQueries",
    "max_items": 10,
    "query_structure": {
      "title": "string",
      "url": "string",
      "data": "ScrapedData",
      "timestamp": "number"
    }
  },
  "performance_metrics": {
    "timing_benchmarks": {
      "main_page_scrape": "2-5s",
      "subdomain_scan": "1-3s each",
      "security_analysis": "<1s",
      "robots_txt_check": "<1s",
      "total_analysis": "3-8s"
    }
  },
  "build_deployment": {
    "npm_commands": {
      "dev": "npm run dev - dev_server",
      "build": "npm run build - production_bundle",
      "preview": "npm run preview - built_app_test"
    },
    "docker_config": {
      "from": "nginx:alpine",
      "copy": "dist/ /usr/share/nginx/html/",
      "expose": "80"
    }
  },
  "ethics_compliance": {
    "principles": [
      "robots_txt_respect: always",
      "terms_of_service: always",
      "rate_limiting: implement",
      "data_minimization: implement",
      "user_agent: ScrapiiBot/2.0"
    ],
    "legal_framework": {
      "GDPR_compliant": "data_minimization",
      "robots_txt": "RFC_9309_compliance",
      "terms_of_service": "automated_analysis",
      "copyright": "fair_use_principles"
    }
  },
  "requestFramePermissions": [],
  "name": "Scrapii"
}